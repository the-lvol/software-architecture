{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "preLaunchTask": "npm: build",
            "cwd": "${workspaceFolder}/deliverable-5/lib",
            "sourceMaps": true,
            "program": "${workspaceFolder}/deliverable-5/lib/out/index.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "outFiles": [
                "${workspaceFolder}/deliverable-5/lib/out/**/*.js"
            ],
        }
    ]
}